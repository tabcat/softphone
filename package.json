{
  "name": "@tabcat/softphone",
  "version": "0.0.1",
  "description": "a basic featured *phone* for you browser",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "clean-cra-sw": "rm -f build/precache-manifest.*.js && rm -f build/service-worker.js",
    "build-sw": "node ./src/sw-build.js",
    "build": "react-scripts build && npm run build-sw && npm run clean-cra-sw",
    "analyze": "echo 'this will take some time... ~15min' && source-map-explorer build/static/js/*.js --html ./source-map-explorer.html",
    "test": "react-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabcat/softphone.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "standard": {
    "ignore": [
      "src/js/components/templates/"
    ]
  },
  "keywords": [
    "p2p",
    "peer-account",
    "ipfs",
    "orbit",
    "orbitdb"
  ],
  "author": "andersb@tuta.io",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tabcat/softphone/issues"
  },
  "homepage": "./",
  "dependencies": {
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@tabcat/ipfs-bundle-t": "0.0.6",
    "@tabcat/peer-account": "0.0.4",
    "@tabcat/peer-account-login": "0.0.6",
    "orbit-db": "^0.22.1",
    "qrcode.react": "^1.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-qr-reader": "^2.2.1",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0"
  },
  "devDependencies": {
    "source-map-explorer": "^2.2.0",
    "workbox-build": "^4.3.1"
  }
}
